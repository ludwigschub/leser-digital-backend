    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node-schedule": "^2.1.7",
    "@types/turndown": "^5.0.5",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "2.0.8",
    "prisma": "^5.22.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",

    "jsdom": "^26.0.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.0.1",
    "node-schedule": "^2.1.1",
    "openai": "^4.95.1",
    "p-queue-cjs": "^7.3.4",

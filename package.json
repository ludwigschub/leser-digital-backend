    "dev:scraper": "ts-node-dev --respawn src/scraper/scraper.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
